@INPROCEEDINGS{Sokolov12nonlinear,
  author = {Artem Sokolov and Guillaume Wisniewski and Fran{\c c}ois Yvon},
  title = {Non-Linear n-best reranking with few features},
  booktitle = {Proceedings of the tenth Conference of the Association for Machine
	Translation in the Americas (AMTA 2012)},
  year = {2012},
  pages = {1--10},
  address = {San Diego, CA},
  abstract = { In Machine Translation, it is customary to compute the model score
	of a predicted hypothesis as a linear combination of multiple features,
	where each feature assesses a particular facet of the hypothesis.
	The choice of a linear combination is usually justified by the possibility
	of efficient inference ; yet, the appropriateness of this simple
	combination scheme to the task at hand is rarely questioned. In this
	paper, we propose an approach that replaces the linear scoring function
	with a non-linear scoring function. To investigate the applicability
	of this approach, we rescore n-best lists generated with a conventional
	machine translation engine with a non-linear scoring function learned
	using the learning-to-rank framework. Moderate, though consistent,
	gains in BLEU are demonstrated on the WMT'10, WMT'11 and WMT'12 test
	sets.},
  bdsk-url-1 = {http://www.mt-archive.info/AMTA-2012-Sokolov.pdf},
  date-added = {2014-02-01 12:06:46 +0000},
  date-modified = {2014-02-01 12:06:46 +0000},
  url = {http://www.mt-archive.info/AMTA-2012-Sokolov.pdf}
}

@INPROCEEDINGS{Sokolov11minimum,
  author = {Artem Sokolov and Fran{\c c}ois Yvon},
  title = {{Minimum Error Rate Semi-Ring}},
  booktitle = {Proceedings of the European Conference on Machine Translation},
  year = {2011},
  editor = {Mikel Forcada and Heidi Depraetere},
  pages = {241--248},
  address = {Leuven, Belgium},
  abstract = {Modern Statistical Machine Translation (SMT) systems make their decisions
	based on multiple information sources, which assess various aspects
	of the match between a source sentence and its possible translation(s).
